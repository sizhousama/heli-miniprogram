    border-radius: 11px;
    height: 15px;
    width: 15px;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
    border-radius: 14px;
    text-align: center;
    line-height: 25px;
    font-size: 14px;
    height: 25px;
    width: 25px;
    background-color:#0094fd;
    color:white;
