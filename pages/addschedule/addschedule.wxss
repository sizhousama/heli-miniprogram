/* pages/addschedule/addschedule.wxss */
page{
  background: #F5F5F5;
}
.duletop{
  background: #fff;
  border-bottom: 2rpx solid #E5E5E5;
}
.dulenav{
  width:100%;
  height: 80rpx;
  line-height: 80rpx;
  background: #FFFFFF;
  border-bottom: 2rpx solid #E5E5E5;
  text-align: center;
  font-size: 30rpx;
  color: #333333;
}
.duletext{
  width:100%;
  padding: 40rpx 24rpx;
  height: 256rpx;
  box-sizing: border-box;
  font-size: 30rpx;
}
.off{
  width:29rpx;
  height: 30rpx;
  position: absolute;
  top: -13rpx;
  right: 0;
}
.off image{
  width:100%;
  height: 100%;
}
.weui-uploader__input {
  background:#F5F5F5;
}


.upbox{
  float:left;
  position:relative;
  margin-right:9px;
  margin-bottom:9px;
  width:160rpx;
  height:160rpx;
  border:1px solid #d9d9d9;
  background:#F5F5F5;
  padding-top: 32rpx;
  box-sizing: border-box;
}

.upicon{
  width:48rpx;
  height: 48rpx;
}
.upbj{
  font-size: 30rpx;
  color: #999999;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.addlx{
  border-top:1px solid #eee;
  border-bottom:1px solid #eee;
  margin-top:20rpx;
  background:#fff;
}
.biaoqian{
width:40rpx;
height:40rpx;
margin-bottom:-3px;
margin-left:10rpx;
}
.setbtn{
  width:500rpx;
  height: 100rpx;
  line-height: 100rpx;
  background-image: linear-gradient(90deg, #00C9FF 0%, #0098FF 100%);
  box-shadow: 0 4px 6px 0 #BEE1FA;
  border-radius: 5px;
  font-size: 16px;
  color: #FFFFFF;
