/* pages/upweituo/upweituo.wxss */
.wt{
  height:60vh
}
.wtitle{
  width:100%;
  height:100rpx;
  line-height: 100rpx;
  padding:0 30rpx;
  font-size: 34rpx;
  border-bottom:1px solid #ddd
}
.wtimgbox{
  margin:20rpx 0
}
.addwt{
  width:200rpx;
  height:200rpx;
  border-radius: 5px;
  margin:20rpx;
  float:left;
  position: relative;
  text-align: center;
  line-height: 280rpx;
  color:#9cbbec;
  font-size: 34rpx

}

.addbg{
  width:200rpx;
  height:200rpx;
  position: absolute;
  left:0;
  top:0;
  z-index:-1;
}
.wtimg{
  width:200rpx;
  height:200rpx;
  border:1px solid #999;
  border-radius: 5px;
  margin:20rpx;
  float:left;
  position: relative;
}

.shanchu{
  width:50rpx;
  height:50rpx;
  position: absolute;
  top:-20rpx;
  right:-20rpx;
}
.wtd{
  width:200rpx;
  height:200rpx;
}