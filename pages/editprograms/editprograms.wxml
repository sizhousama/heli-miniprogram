                        <view class="weui-select weui-select_in-select-after">{{yharr[YS_obja.data.yanghu]}}</view>
                    </picker>
                </view>
            </view>
            <view class="weui-cell weui-cell_access" style='height:0'>
            </view>
            <view class="bottom"></view>
            
        </view>
        <view class="bottom"></view>
        <view class="botbox " style=''>
               <button type='button' class="storbtn" bindtap="savePro" bindtap="savePro">保存</button>
          </view>
    </view>


