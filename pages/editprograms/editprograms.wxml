                    <view class="weui-label">竣工日期</view>
                </view>
                <view class="weui-cell__bd">
                    <picker mode="date" value="{{YS_obja.data.jungong}}" start="2015-09-01" end="2022-09-01" bindchange="bindendChange">
                        <view class="weui-input weui-cell__ft_in-access" style='display:inherit;margin-right:10rpx;'>{{endtime?endtime:YS_obja.data.jungong}}</view>
                    </picker>
                </view>
            </view>

         

            <view class="weui-cell weui-cell_select">
